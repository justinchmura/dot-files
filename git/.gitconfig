[user]
  name = Justin Chmura
  email = justin.chmura@gmail.com
[github]
  user = justinchmura
[credential]
  helper = osxkeychain
[core]
  editor = vim -f
  excludesfile = /Users/chris/.global_gitignore
[alias]
  lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr, %cn)%Creset' --abbrev-commit --date=relative
  hist = log --graph --pretty=format:'%h %ad | %s%d [%an]' --date=short
  last = log -1 HEAD
  unstage = reset HEAD --
  amend = commit --amend -C HEAD
  co = checkout
  ci = commit
  st = status
  br = branch
